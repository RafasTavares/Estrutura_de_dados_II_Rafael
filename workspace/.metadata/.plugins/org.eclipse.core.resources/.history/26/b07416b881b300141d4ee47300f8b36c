package Tree;

public class TreeNode<T extends Comparable<T>> {

	int data;
	TreeNode leftChild;
	TreeNode rightChild;
	TreeNode centerChild;

	/**
	 * Node Constructor
	 * 
	 * @param dataValue
	 *            the value of that given Node
	 */
	public TreeNode(int dataValue) {
		data = dataValue;
		leftChild = null;
		rightChild = null;
		centerChild = null;
	}
}