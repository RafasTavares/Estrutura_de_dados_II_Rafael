package Tree;

public class TreeNode<T extends Comparable<T>> {
	/// Variáveis
	T valor;
	TreeNode<T> pai;
	TreeNode<T> filhos;
	
	
	public T getValor() {
		return valor;
	}
	public void setValor(T valor) {
		this.valor = valor;
	}
	public TreeNode<T> getPai() {
		return pai;
	}
	public void setPai(TreeNode<T> pai) {
		this.pai = pai;
	}
	public TreeNode<T> getFilhos() {
		return filhos;
	}
	public void setFilhos(TreeNode<T> filhos) {
		this.filhos = filhos;
	}
}
