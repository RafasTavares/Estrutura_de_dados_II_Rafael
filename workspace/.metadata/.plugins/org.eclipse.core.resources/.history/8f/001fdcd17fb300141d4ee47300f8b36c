package Tree;


public class TreeNode<T extends Comparable<T>> {

	private class Node {
		int data;
		Node leftChild;
		Node rightChild;
		Node centerChild;

		/**
		 * Node Constructor
		 * 
		 * @param dataValue
		 *            the value of that given Node
		 */
		public Node(int dataValue) {
			data = dataValue;
			leftChild = null;
			rightChild = null;
			centerChild = null;
		}
	}