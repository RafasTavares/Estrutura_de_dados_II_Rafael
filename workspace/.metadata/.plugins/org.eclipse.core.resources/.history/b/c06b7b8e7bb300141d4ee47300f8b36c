package Tree;

public class TreeNode<T extends Comparable<T>> {
	
    // VARIAVEIS
	TrieNode esquerdaNode;
	int info;
	TrieNode direitaNode; 
	
	//CONSTRUTOR
	public TrieNode(int nodeInfo) {
		info = nodeInfo;
		esquerdaNode = direitaNode = null;
	}

	// INSERE NO
	public void insereNode(int insertValue) {
		if (insertValue < info) {
			//
			if (esquerdaNode == null)
				esquerdaNode = new TrieNode(insertValue);
			//
			else
				esquerdaNode.insereNode(insertValue);
			//
		} else if (insertValue > info) {
			//
			if (direitaNode == null)
				direitaNode = new TrieNode(insertValue);
			//
			else
				direitaNode.insereNode(insertValue);
		}
	}
}
